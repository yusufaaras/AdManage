<!DOCTYPE html>
<html lang="en">
@await Html.PartialAsync("~/Views/AdminUI/_HeaderPartial.cshtml")

<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Navbar -->
        @await Html.PartialAsync("~/Views/AdminUI/_NavbarPartial.cshtml")
        
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        @await Html.PartialAsync("~/Views/AdminUI/_SidebarPartial.cshtml")

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h1>DataTables</h1>
                        </div>
                        <div class="col-sm-6">
                            <ol class="breadcrumb float-sm-right">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item active">DataTables</li>
                            </ol>
                        </div>
                    </div>
                </div><!-- /.container-fluid -->
            </section>
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        @await Html.PartialAsync("~/Views/AdminUI/_FooterPartial.cshtml")

        <aside class="control-sidebar control-sidebar-dark">
        </aside>
    </div>

    @await Html.PartialAsync("~/Views/AdminUI/_ScriptPartial.cshtml")
</body>
</html>
